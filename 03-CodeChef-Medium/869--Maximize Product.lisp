;;;
;;; Body
;;;

(defun main ()
  (declare #.*opt*)
  (let* ((tt (read)))
    (write-string
     (with-output-to-string (*standard-output* nil :element-type 'base-char)
       (dotimes (_ tt)
         (block continue
           (let ((n (read))
                 (k (read)))
             (declare (uint31 n k))
             (when (< n (ash (* k (+ k 1)) -1))
               (println -1)
               (return-from continue))
             (let* ((base (sb-int:named-let bisect ((ok 0) (ng n))
                            (declare (uint31 ok ng))
                            (if (<= (- ng ok) 1)
                                ok
                                (let ((mid (ash (+ ok ng) -1)))
                                  (if (<= (+ (* mid k) (ash (* k (- k 1)) -1))
                                          n)
                                      (bisect mid ng)
                                      (bisect ok mid))))))
                    (dif (- n (+ (* base k)
                                 (ash (* k (- k 1)) -1))))
                    (res 1))
               (declare (uint31 res base))
               (loop for x from base to (+ base k)
                     unless (= x (- (+ base k) dif))
                     do (mulfmod res (mod* x (max 0 (- x 1)))))
               (println res)))))))))

#-swank (main)

